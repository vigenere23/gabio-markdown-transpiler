{
  "name": "@gabio/markdown-vue-transpiler",
  "version": "0.0.1",
  "description": "A markdown to Vue transpiler",
  "publishConfig": {
    "access": "public"
  },
  "main": "index.js",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "example": "ts-node example/main.ts",
    "prepublishOnly": "yarn build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vigenere23/markdown-vue-transpiler.git"
  },
  "keywords": [
    "vue",
    "markdown"
  ],
  "author": "Gabriel St-Pierre",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vigenere23/markdown-vue-transpiler/issues"
  },
  "homepage": "https://github.com/vigenere23/markdown-vue-transpiler#readme",
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/marked": "^1.1.0",
    "@types/node": "^14.11.2",
    "@vigenere23/gio": "^1.8.1-beta.0",
    "prettier": "^2.1.2",
    "ts-node": "^9.0.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.0.3",
    "vue": "^2.6.12",
    "vue-property-decorator": "^9.0.0"
  },
  "dependencies": {
    "glob": "^7.1.6",
    "marked": "^1.1.1"
  }
}
